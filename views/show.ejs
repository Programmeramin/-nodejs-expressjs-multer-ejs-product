<div class="container">
  <div class="row justify-content-center my-5">
    <div class="col-md-10">
      <a href="/create" class="btn btn-sm btn-primary mb-3">Add new product</a>
      <div class="card">
        <div class="card-body">
          <img class="w-100" src="/product/<%= product.photo %>" alt="" />

          <h1><%= product.name %></h1>
          <h1><%= product.regularPrice %></h1>
        </div>
      </div>
    </div>
  </div>
</div>
